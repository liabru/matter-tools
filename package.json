{
  "name": "matter-tools",
  "version": "0.11.1",
  "license": "MIT",
  "homepage": "https://github.com/liabru/matter-tools",
  "author": "Liam Brummitt <liam@brm.io> (http://brm.io/)",
  "description": "tools for creating, testing and debugging matter.js worlds",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/liabru/matter-tools.git"
  },
  "keywords": [
    "javascript",
    "canvas",
    "html5",
    "physics",
    "physics engine",
    "game engine",
    "rigid body physics"
  ],
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --content-base docs/",
    "lint": "eslint *.js src/**/*.js test/*.js",
    "doc": "markdox *.js src/**/*.js -o API.md",
    "release": "npm version minor -m 'release %s' && npm publish",
    "release-patch": "npm version patch -m 'release %s' && npm publish",
    "preversion": "npm run lint",
    "version": "npm run build && npm run doc && git add -A build docs API.md",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "fs-extra": "^9.0.1",
    "jquery": "^3.1.1",
    "jstree": "^3.3.3",
    "keymaster": "^1.6.2",
    "matter-js": "^0.12.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "case": "^1.4.1",
    "chai": "^3.5.0",
    "eslint": "^3.9.1",
    "exports-loader": "^0.6.3",
    "markdox": "^0.1.10",
    "mocha": "^3.1.2",
    "raw-loader": "^0.5.1",
    "string-replace-loader": "^1.0.5",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
